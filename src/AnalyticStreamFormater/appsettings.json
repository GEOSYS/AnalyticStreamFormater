{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Azure": {
    "BlobStorage": {
      "ConnectionString": "MyAzureBlobStorageConnectionString"
        }
  },
  "IdentityServer": {
    "Url": "IdentityServerUrl",
    "TokenEndPoint": "connect/token",
    "UserLogin": "myuser",
    "UserPassword": "mypassword",
    "ClientId": "myclientid",
    "ClientSecret": "myclientsecret",
    "Scope": "openid offline_access",
    "GrantType": "password"
  },
  "MapProduct": {
    "Url": "MapProductUrl"
  }
}
